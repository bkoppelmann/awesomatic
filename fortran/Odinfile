  (Specifications/f77.exe) :run

%try90f ! == . +cmd=(F90Tests/try90fix) (F90Tests/f90fixed) \
  (Specifications/f90.exe) :run

%try90v ! == . +cmd=(F90Tests/try90var) (F90Tests/f90variable) \
  (Specifications/f90.exe) :run

%try90i ! == . +cmd=(F90Tests/try90var) (F90Tests/f90include) \
  (Specifications/f90.exe) :run
